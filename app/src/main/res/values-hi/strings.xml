    <!-- Dialog Descriptions -->
    <string name="add_books_description">
        आप चुन सकते हैं कि आप कौन सी पुस्तकें जोड़ना चाहते हैं।
        आप बाद में पुस्तक का शीर्षक और कवर छवि संपादित कर सकते हैं।
        लोड होने की प्रक्रिया में कुछ मिनट तक लग सकते हैं.
    </string>
    <string name="move_books_description">
        उस श्रेणी को चुनें जिसमें आप सभी चयनित पुस्तकों (%1$s) को ले जाना चाहते हैं।
        सभी चयनित पुस्तकें चयनित श्रेणी में ले जाई जाएँगी।
    </string>
    <string name="delete_books_description">
        यह सभी चयनित पुस्तकों (%1$s) को डेटाबेस से मिटा देगा।
        यह आपके डिवाइस से चयनित पुस्तकों को नहीं मिटाएगा।
    </string>
    <string name="delete_book_description">
        यह इस पुस्तक को डेटाबेस से मिटा देगा।
        यह आपके डिवाइस से इस पुस्तक को नहीं मिटाएगा।
    </string>
    <string name="move_book_description">
        उस श्रेणी को चुनें जिसमें आप इस पुस्तक को ले जाना चाहते हैं।
        यह पुस्तक चयनित श्रेणी में ले जाई जाएगी।
    </string>
    <string name="delete_history_description">
        यह डेटाबेस से पूरा इतिहास मिटा देगा।
        पूरा इतिहास मिटाने के बाद, पुस्तकों का क्रम रीसेट हो जाएगा।
    </string>
    <string name="update_app_description">
        ऐप का नया संस्करण मिला।
        क्या आप आगे बढ़ना और रिलीज़ पेज पर जाना चाहते हैं?
    </string>
    <string name="enable_check_for_updates_description">
        «अपडेट के लिए जांचें» को सक्षम करने से
        ऐप हर बार «api.github.com» से नवीनतम रिलीज़ जानकारी प्राप्त करेगा
        जब ऐप शुरू होता है।
        क्या आप आगे बढ़ना और «अपडेट के लिए जांचें» सक्षम करना चाहते हैं?
    </string>

